<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#1a000000" android:endColor="#1a000000" android:angle="270.0" android:centerY="0.75" android:centerColor="#1a000000" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#1a000000" android:endColor="#1a000000" android:angle="270.0" android:centerY="0.75" android:centerColor="#1a000000" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ff2076ea" android:endColor="#ff2076ea" android:angle="270.0" android:centerY="0.75" android:centerColor="#ff2076ea" />
            </shape>
        </clip>
    </item>
</layer-list>