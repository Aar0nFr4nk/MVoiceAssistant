<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ff167fce" android:endColor="#ff0d60c9" android:angle="180.0" />
            <corners android:radius="25.0dip" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="#ff167fce" android:endColor="#ff0d60c9" android:angle="180.0" />
            <corners android:radius="25.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ff1c99f3" android:endColor="#ff106eed" android:angle="180.0" />
            <corners android:radius="25.0dip" />
        </shape>
    </item>
</selector>